            getQueryString: function (name) {
                var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
                var r = window.location.search.substr(1).match(reg);
                if (r != null) {
                    return unescape(r[2]);
                }
                return '';
            },
            setTid: function () {
                var tid = this.getQueryString('tid') || ''
                if (tid == 'fromSCnmmBanner') {
                    unitLoaded && Unit.sendGA('Homepage_Open_From_SMbanner')
                    setLocalStorage('fromSCnmmBanner', 1);
                }
                if (tid.length > 0) {
                    setCookie('tid', tid, { path: '/', domain: getRootDomain(), expires: 14 })
                    if (typeof window.location.search.substring(1) == 'string' && window.location.search.substring(1) != 'tid=' + tid) {
                        setCookie('tidAffiliateParams', window.location.search.substring(1), { path: '/', domain: getRootDomain(), expires: 14 })
