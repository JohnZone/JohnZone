                }
                var queryEmail = this.getQueryString('email') || ''
                var sendTime = this.getQueryString('sendTime') || ''
                var emailTag = this.getQueryString('emailTag') || ''
                if (queryEmail) {
                    let submitData = {
                        email: queryEmail || '',
                        emailTag: emailTag || '',
                        sendTime: sendTime || '',
                        type: 2
                    }
                    reportEmail(submitData)
                }
                var cjevent = this.getQueryString('cjevent') || ''
                if (cjevent) {
                    setTimeout(() => {
                        if (!window.cj) window.cj = {};
                        let referringChannel = getCookie('referrerPage') ? getCookie('referrerPage') : ''
                        cj.sitePage = {
                            enterpriseId: 1566360,
                            pageType: 'homepage',
                            referringChannel: referringChannel
                        };
                    }, 200);
                }
                var t2 = this.getQueryString('t2') || ''
                if (t2) {
                    setCookie('t2', t2, { path: '/', domain: getRootDomain(), expires: 14 })
                }
                var referrerPage = this.getQueryString('referrer_page') || ''
                if (referrerPage) {
                    setCookie('referrerPage', referrerPage, { path: '/', domain: getRootDomain(), expires: 14 })
                }
                var trafficSource = this.getQueryString('traffic_source') || ''
                if (trafficSource) {
                    setCookie('trafficSource', trafficSource, { path: '/', domain: getRootDomain(), expires: 14 })
                }
                var providerName = this.getQueryString('provider_name') || ''
                if (providerName) {
                    setCookie('providerName', providerName, { path: '/', domain: getRootDomain(), expires: 14 })
                }
            },
            syncLogin: function () {
                if (getCookie('clientKey')) {
                    window.location.href = isMobile ? '/spark' : '/myHome'
                } else {
                    setTimeout(() => {
                        document.getElementById('pageLoading') && document.getElementById('pageLoading').remove()
                    }, 300)
                }
            },
            setUUID: function () {
                if (!getCookie('UUID')) {
                    let key = 'click_report'
                    currentLocationStr = getCookie('sign_key'),
                        options = {
                            path: '/',
                            domain: getRootDomain(),
                            expires: 1000
                        }
                    setCookie('UUID', md5(new Date().getTime() + Math.ceil(Math.random() * 10000000).toString() + currentLocationStr), options)
                }
            },
            changeLanguage(language) {
                if (language == '/en/en') {
                    setCookie('googtrans', '', { path: '/', expires: -1 })
                    setCookie('googtrans', '', { path: '/', domain: getRootDomain(), expires: -1 })
                } else {
                    setCookie('googtrans', language, { path: '/', expires: 0 })
                    setCookie('googtrans', language, { path: '/', domain: getRootDomain(), expires: 0 })
                }
                unitLoaded && Unit.sendGA('Multilanguage_Page_Click_beforelogin')
                window.location.reload()
            },
        },
    }
    function reportTid(tidNum) {
        ajax({
            url: 'click_report',
