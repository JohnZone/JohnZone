    /**
     * @description: Initializes the Facebook meta-statistics code
     * @param {*} id Meta Id
     * @return {*} No return value
     */    
     function initFbMeta(id) {
        if (!id) return
        !(function (f, b, e, v, n, t, s) {
            if (f.fbq) return
            n = f.fbq = function () {
                n.callMethod ? n.callMethod.apply(n, arguments) : n.queue.push(arguments)
            }
            if (!f._fbq) f._fbq = n
            n.push = n
            n.loaded = !0
            n.version = '2.0'
            n.queue = []
            t = b.createElement(e)
            t.async = !0
            t.src = v
            s = b.getElementsByTagName(e)[0]
            s.parentNode.insertBefore(t, s)
        })(window, document, 'script', 'https://connect.facebook.net/en_US/fbevents.js')

        fbq('init', id)
        fbq('track', 'PageView')

        var fbMetaNoScript = document.createElement('noscript'),
            fbMetaNoScriptImg = document.createElement('img')
        fbMetaNoScriptImg.setAttribute('height', '1')
        fbMetaNoScriptImg.setAttribute('width', '1')
        fbMetaNoScriptImg.setAttribute('style', 'display:none')
        fbMetaNoScriptImg.setAttribute('src', `https://www.facebook.com/tr?id=${id}&ev=PageView&noscript=1`)
        fbMetaNoScript.appendChild(fbMetaNoScriptImg)
        document.body.appendChild(fbMetaNoScript)
    }
