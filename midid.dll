    function changeDataType(obj) {
        let str = ''
        if (typeof obj == 'object') {
            for (let i in obj) {
                if (typeof obj[i] != 'function' && typeof obj[i] != 'object') {
                    str += i + '=' + encodeURIComponent(obj[i]) + '&';
                } else if (typeof obj[i] == 'object') {
                    nextStr = '';
                    str += changeSonType(i, obj[i])
                }
            }
        }
        return str.replace(/&$/g, '');
    }

    function changeSonType(objName, objValue) {
        if (typeof objValue == 'object') {
            for (let i in objValue) {
                if (typeof objValue[i] != 'object') {
                    let value = objName + '[' + i + ']=' + objValue[i];
                    nextStr += encodeURI(value) + '&';
                } else {
                    changeSonType(objName + '[' + i + ']', objValue[i]);
                }
            }
        }
        return nextStr;
    }

    function getAjaxHeader(data, method, apiName) {
        var isMobile = (/(Android|webOS|iPhone|iPad|iPod|BlackBerry|Windows Phone)/i.test(navigator.userAgent) && "ontouchend" in document) && document.body.clientWidth < 768 ? true : false,
            t = parseInt(new Date().getTime() / 1000),
            signKey = getCookie('sign_key') || md5(Math.ceil(Math.random() * 10000000).toString()),
            nonce = Math.ceil(Math.random() * 999),
            paramStr = changeDataType(data).replace(/\%20/g, '+'),
            signature = '',
            uuid = getCookie('UUID') || md5(Math.ceil(Math.random() * 1000000).toString()),
            specialApis = ['upload_picture', 'upload_file', 'upload_timeline'],
            specialParameters = ['file', 'url'];
        if (Object.prototype.toString.call(data) === '[object FormData]') {
            var newkey = this.getFormDataKeys(data),
                formData = {};
            paramStr = '';
            for (var i = 0; i < newkey.length; i++) {
                if (specialApis.indexOf(apiName) == -1 || (specialApis.indexOf(apiName) > -1 && specialParameters.indexOf(newkey[i]) == -1)) {
                    formData[newkey[i]] = data.get(newkey[i])
                }
            }
            paramStr = changeDataType(formData).replace(/\%20/g, '+')
        }

        signature = (md5(signKey + paramStr + nonce + t)).toUpperCase();

        if (!signKey) {
            console.log('Abnormal request.');
            return false;
        }

        if (Env == 'dev') {
            return {
                'ParamStr': paramStr,
            }
        }
        return {
            'Sign-Key': signKey,
            'Timestamp': t,
            'uuid': uuid,
            'Nonce': nonce,
            'Signature': signature,
            'devicetype': isMobile ? 4 : 3,
        }
    };

    function ajax(obj) {
        obj.type = obj.type || "get";
        obj.async = obj.async || true;
        obj.data = obj.data || null;
        var params = _params(obj.data);
        var host = window.location.hostname;
        var domain = window.location.origin;
        var apiUrl = 'https://' + host + '/api/v1/';
        if (new RegExp('de4\..+?\.com').test(host)) {
            apiUrl = domain + '/api/';
        }
        obj.url = apiUrl + obj.url;

        if (obj.type.toLowerCase() == "get" && params.length > 0) {
            obj.url += "?" + params;
        }

        var xhr = new XMLHttpRequest();
        xhr.open(obj.type, obj.url, obj.async);

        var head = getAjaxHeader(obj.data, obj.type, obj.url);

        if (Env == 'dev') {
            xhr.setRequestHeader("Token", getCookie('token'));
        }
        // if(obj.type.toLowerCase()=="post"){
        xhr.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
        for (var i in head) {
            xhr.setRequestHeader(i, head[i]);
        }
        xhr.send(params)
        // }else{
        //     xhr.send(null);
        // }
        if (obj.async) {
            xhr.onreadystatechange = function () {
                if (xhr.readyState == 4)
                    callback();
            }
        } else
            callback();
        function callback() {
            if (xhr.status == 200) {
                obj.success(JSON.parse(xhr.responseText));
            } else {
                obj.error && obj.error(xhr.status);
            }
            obj.complete && obj.complete(xhr)
        }
        function _params(data) {
            if (obj == null)
                return obj;
            var arr = [];
            for (var i in data) {
                arr.push(encodeURIComponent(i) + '=' + encodeURIComponent(data[i]));
            }
            return arr.join("&");
        }
    }

