            }
        })
    }
    function reportEmail(submitData) {
        ajax({
            url: 'click_report',
            type: 'post',
            data: submitData,
            success: function (data) {
            }
        })
    }
    if (Env == 'dev') {
        ajax({
            url: 'get_token',
            method: 'get',
            data: {},
            success: function (response, status, xhr) {
                if (response.access_token) {
                    setCookie('token', response.access_token, { path: '/', domain: getRootDomain(), expires: 3 })
                    global.methods.init();
                    loadingXhrLoaded = true;
                } else {
                    console.log('error:', response)
                }
            }
        })
    } else {
        let cjEvent = global.methods.getQueryString('cjevent') || ''
        let cjTid = global.methods.getQueryString('tid') || ''
        let xhr = new XMLHttpRequest()
        xhr.onreadystatechange = () => {
            if (xhr.readyState == 4) {
                global.methods.init();
                loadingXhrLoaded = true;
            }
        }
        xhr.open('get', window.location.origin + `/loading?cjevent=${cjEvent}&tid=${cjTid}`, true)
        xhr.send(null)
    }
