  document.addEventListener('input', function(event) {
      if (event.target.id === 'ConvertToRunes') {
          const inputArray = replaceSpelling(event.target.value).toLowerCase().split('');
          const runeTypes = [elderArray, youngerArray, shortTwigArray, noTwigArray, angloArray, medievalArray];
          const outputs = ['elder', 'younger', 'shorttwig', 'notwig', 'anglo', 'medieval'];
  
          runeTypes.forEach((array, index) => {
              document.querySelector('#' + outputs[index]).innerHTML = inputArray.map(letter => getRunes(letter, array)).join('');
          });
      }
  });
  
  function getRunes(letter, array) {
      try {
          const match = array.find(a => a.letters.includes(letter));
          return match ? match.rune : `<span style="color: red">${letter.toUpperCase()}</span>`;
      } catch (e) {
          return `<span style="color: red">${letter.toUpperCase()}</span>`;
      }
  }
  
  function replaceSpelling(input) {
      input = input.replace(/th/g, 'þ').replace(/x/g, 'ks').replace(/ae/g, 'æ')
                   .replace(/ia/g, 'õ').replace(/io/g, 'ö').replace(/ea/g, 'ä')
                   .replace(/ng/g, 'ŋ');
      const lastChar = input[input.length - 1];
      if (lastChar === 'r') input = input.slice(0, -1) + 'ʀ';
      return input;
  }
  
