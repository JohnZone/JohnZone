        if(rune == "SPACE"){
          inputElement.value = originalValue + " ";
        }else{
          inputElement.value = originalValue + rune;
        }
        
        const inputEvent = new InputEvent('input', {
            bubbles: true,   // Allows the event to bubble up through the DOM
            cancelable: false, // Typically, input events aren't cancelable
            data: rune,      // Data property can specify what was added to the input
            inputType: 'insertText', // Describes the type of operation that caused the event
            view: window,    // Reference to the window object
            composed: true   // Can cross shadow DOM boundaries if necessary
        });

        inputElement.dispatchEvent(inputEvent);
      }
  });
  
  document.addEventListener('input', function(event) {
      if (event.target.id === 'ConvertToText') {
          const text = event.target.value.split('');
          const translatedText = text.map(char => {
              const match = elderArray.find(a => a.rune === char)
                ?? youngerArray.find(a => a.rune === char)
                ?? shortTwigArray.find(a => a.rune === char)
                ?? noTwigArray.find(a => a.rune === char)
                ?? medievalArray.find(a => a.rune === char)
                ?? angloArray.find(a => a.rune === char);
            
              return match ? match.letters[0] : `<span style="color: red">${char}</span>`;
          }).join('');
          document.querySelector('#ConvertToTextResult').innerHTML = translatedText;
      }
  });
  
