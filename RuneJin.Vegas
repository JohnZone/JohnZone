          { id: '#AngloRuneKeyboard', array: angloArray },
      ];
  
      runeKeys.forEach(item => {
          const container = document.querySelector(item.id);
          if (container) {
              item.array.forEach(rune => {
                  const button = document.createElement('button');
                  button.className = 'btn btn-secondary btn-flat bg-light rune-btn';
                  button.textContent = rune.rune;
                  container.appendChild(button);
              });
          } else {
              console.error('Container not found for ID:', item.id);
          }
      });
  }
  
  document.addEventListener('click', function(event) {
