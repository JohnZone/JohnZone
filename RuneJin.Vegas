  document.addEventListener('DOMContentLoaded', function() {
    // Targeting links within li elements having data-parentid
    const menuLinks = document.querySelectorAll('li[data-parentid] .navbar-secondary-link');
  
    menuLinks.forEach(link => {
      link.addEventListener('mouseover', function() {
        // Fetch the parentid attribute to identify the corresponding main image
        const parentID = this.closest('li[data-parentid]').getAttribute('data-parentid');
        const newImage = this.closest('li[data-parentid]').getAttribute('data-image');
        const mainImage = document.getElementById(parentID);
  
        if (mainImage && newImage) {
          mainImage.src = newImage;
        }
      });
  
      link.addEventListener('mouseout', function() {
        const parentID = this.closest('li[data-parentid]').getAttribute('data-parentid');
        const mainImage = document.getElementById(parentID);
        const mainImageDefault = mainImage.getAttribute('data-originalimage');
  
        if (mainImage) {
          mainImage.src = mainImageDefault;
        }
      });
    });
  });
</script>
