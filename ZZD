<script type="text/javascript">
    
        $(document).ready(function(){
            let val = {};

            deleteCookie("Subscribe.Companies");

            if($('.company-newsletter-btn').length > 0){
                $('.company-newsletter-btn').click(function(){
                    var companyCode = $(this).attr('data-id');
                    var companyName = $(this).attr('data-name');
                        val = getCookie("Subscribe.Companies");
                        val = (val) ? JSON.parse(val) : {};
                        val[companyCode] = companyName;

                        setCookie("Subscribe.Companies", JSON.stringify(val), {domain: 'vz.lt', path: '/'});

                        $('.rekvizitai-modal__body iframe').attr('src', 'https://mano.vz.lt/imones');
                });
            }

            function setCookie(name, value, opts = {}){
                /*If options contains days then we're configuring max-age*/
                if (opts.days) {
                    opts['max-age'] = opts.days * 60 * 60 * 24;

                    /*Deleting days from options to pass remaining opts to cookie settings*/
                    delete opts.days
                }

                /*Configuring options to cookie standard by reducing each property*/
                opts = Object.entries(opts).reduce(
                    (accumulatedStr, [k, v]) => `${accumulatedStr}; ${k}=${v}`, ''
                )

                /*Finally, creating the key*/
                document.cookie = name + '=' + encodeURIComponent(value) + opts
            }

            function getCookie(name){
                let c = document.cookie.match(`(?:(?:^|.*; *)${name} *= *([^;]*).*$)|^.*$`)[1]
                        if (!c) return false;
                        return decodeURIComponent(c);
            }

            function deleteCookie(name, opts){
                setCookie(name, '', {'max-age': -1, ...opts})
            }
        });
    
</script>    
